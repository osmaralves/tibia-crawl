{
  "name": "highscores-scraper",
  "version": "0.0.0",
  "author": "Osmar Alves <osmar.wd@gmail.com>",
  "description": "Tibia highscores scraper",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src && prettier --check 'src/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "PORT=4000 sucrase-node src",
    "build": "rm -rf dist && sucrase ./src -d ./dist --transforms typescript,imports",
    "start": "PORT=4000 node ./dist"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "node-fetch": "^3.2.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node-fetch": "^2.6.2",
    "sucrase": "^3.24.0",
    "typescript": "^4.7.4",
    "eslint-config-custom": "*"
  }
}
